    ViewBag.Title = "Home Page - Checking hook";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
            <p>
                Jenkins, Git, and Visual Studio 

                <b>Continuous Integration Demo</b>
